#include <stdio.h>

int main ()
{

  //kamus 

  float y;
  float v0;
  float t;
  const float g = 9.8;

  //algoritma
  printf("==========Menghitung jarak parabola=========\n");
  printf("============================================\n");
  printf("\nMasukkan nilai v0 = "); 
  scanf("%f",&v0);
  printf("\nMasukkan nilai t = "); 
  scanf("%f",&t);

  y = v0 * t - 0.5 * (g * t * t);

  printf("============================================\n");
  printf("Hasil menghitung parabola adalah = %f",y);
  printf("\n============================================");

return 0;
}